# list git branches by most recently modified
alias glist='for ref in $(git for-each-ref --sort=-committerdate --format="%(refname)" refs/heads/ refs/remotes ); do git log -n1 $ref --pretty=format:"%Cgreen%cr%Creset %C(yellow)%d%Creset %C(bold blue)<%an>%Creset%n" | cat ; done | awk '"'! a["'$0'"]++'"

# Go down a directory, if there is a single directory, keep going. If there is more than 1 file/directory, stop.
# this is useful for going through Java folders.
function scd()
{
	START=`pwd`
	cd $1 || exit 1
	while [ `ls -1 | wc -l` -eq 1 ]; do
		if [ ! -d `ls -1` ]; then
			break
		fi

		cd `ls -1`
	done
	END=`pwd`
	cd $START
	cd $END
}

. `brew --prefix`/etc/profile.d/z.sh
